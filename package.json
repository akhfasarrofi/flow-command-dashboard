{
  "dependencies": {
    "@react-router/fs-routes": "^7.9.4",
    "@react-router/node": "7.10.1",
    "@react-router/serve": "7.10.1",
    "isbot": "^5.1.31",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "7.10.1",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@react-router/dev": "7.10.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "react-router-devtools": "^5.0.6",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-imagetools": "^8.0.0",
    "vite-plugin-inspect": "^11.3.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": "20.x",
    "pnpm": "^10.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,jsonc,css,scss,md}": [
      "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "name": "trade-bot",
  "private": true,
  "scripts": {
    "build": "react-router typegen && tsc && react-router build",
    "check": "biome check --write",
    "check:report": "biome check --unsafe --reporter=summary",
    "dev": "react-router dev",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "lint:unsafe": "biome lint --write --unsafe",
    "prepare": "husky",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "type": "module"
}
